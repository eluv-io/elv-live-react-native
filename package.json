{
  "name": "reactNativeTvos",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "tvos": "react-native run-ios --simulator 'Apple TV' --scheme reactNativeTvos-tvOS",
    "iosdevices": "xcrun simctl list devices",
    "start": "react-native start",
    "reset": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@expo/browser-polyfill": "^0.1.0",
    "@react-navigation/stack": "^5.11.0",
    "dateformat": "^3.0.3",
    "elv-components-js": "git+https://github.com/eluv-io/elv-components-js.git#645165da87ec42845a908a9ce0817db202523866",
    "react": "16.13.1",
    "react-native": "npm:react-native-tvos@0.63.1-2",
    "react-native-fast-image": "^8.3.2",
    "react-native-get-random-values": "^1.5.0",
    "react-native-modal": "^11.5.6",
    "react-native-unimodules": "^0.11.0",
    "react-native-video": "^5.1.0-alpha8",
    "stream": "0.0.2",
    "urijs": "^1.19.2",
    "webassemblyjs": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.5.2",
    "eslint": "^7.10.0",
    "jest": "^26.5.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "node-sass": "^4.14.1",
    "react-native-sass-transformer": "^1.4.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
